inherit python

DESCRIPTION="APE 2.0 tagger"
HOMEPAGE="http://muth.org/Robert/Apetag/"
SRC_URI="http://muth.org/Robert/Apetag/${PN}.${PV}.tar.gz"
SRC_DIR="Apetag"

PATCH_URI="1.12-execinfo.h.patch"

src_compile() {
	lndirs
	cd ${B}
	cygmake
}

src_install() {
	cd ${B}
	dobin apetag.exe *.py
	dodoc 00*
}
